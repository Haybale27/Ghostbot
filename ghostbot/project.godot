; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="ghostbot"
run/low_processor_mode=true
config/icon="res://ghostbot_ghost-1.png"

[autoload]

player_stats="*res://Player/player_stats.gd"

[debug]

settings/stdout/print_fps=true

[display]

window/stretch/mode="2d"
window/stretch/aspect="keep"

[layer_names]

2d_render/layer_1="World"
2d_render/layer_2="Ghostly"
2d_render/layer_3="PlayerBot"
2d_render/layer_4="PlayerGhost"
2d_render/layer_5="EnemyBot"
2d_render/layer_6="EnemyGhost"
2d_render/layer_7="Hazard"
2d_physics/layer_1="World"
2d_physics/layer_2="Ghostly"
2d_physics/layer_3="PlayerBot"
2d_physics/layer_4="PlayerGhost"
2d_physics/layer_5="EnemyBot"
2d_physics/layer_6="EnemyGhost"
2d_physics/layer_7="Hazard"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
